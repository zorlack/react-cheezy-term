{
  "name": "react-cheezy-term",
  "version": "0.0.5",
  "description": "A tongue-in-cheek computer terminal packaged as a UI component for React",
  "author": "zorlack",
  "keywords": [
    "react",
    "terminal",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zorlack/react-cheezy-term.git"
  },
  "bugs": {
    "url": "https://github.com/zorlack/react-cheezy-term/issues"
  },
  "homepage": "https://github.com/zorlack/react-cheezy-term#readme",
  "private": false,
  "type": "module",
  "main": "dist/react-cheezy-term.cjs.js",
  "module": "dist/react-cheezy-term.esm.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-cheezy-term.esm.js",
      "require": "./dist/react-cheezy-term.cjs.js"
    }
  },
  "files": [
    "dist",
    "src/assets/images"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.build.json && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "dependencies": {
    "@xterm/xterm": "^5.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vitest": "^2.1.8"
  }
}
